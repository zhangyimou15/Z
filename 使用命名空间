#include<iostream>
#include<cstdio>
using namespace std;
namespace 帅哥
{
	char name1[20] = "zhangyimou";
	char name2[20] = "wangzizhu";
}
int main() 
{

	cout << 帅哥::name1 << endl;
	cout << 帅哥::name2 << endl;

}

/*为了不使用帅哥::前缀，故在主函数中使用帅哥命名空间*/
#include<iostream>
#include<cstdio>
using namespace std;
namespace 帅哥
{
	char name1[20] = "zhangyimou";
	char name2[20] = "wangzizhu";
}
int main() 
{
	using namespace 帅哥;
	cout << name1 << endl;
	cout << name2 << endl;

}
